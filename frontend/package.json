  "scripts": {
    "start": "react-scripts start",
    "deploy": "cd .. && yarn deploy",
    "build": "react-scripts build && yarn sw-epilog && yarn brotli && yarn gzip && yarn imageOptim",
    "sw-epilog": "cat src/sw-epilog.js >> build/service-worker.js",
    "brotli": "gulp brotli",
    "gzip": "gulp gzip",
    "imageOptim": "gulp imageOptim",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "makeComponent": "bin/makeComponent",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.12",
    "prettier": "1.19.1"
  }
